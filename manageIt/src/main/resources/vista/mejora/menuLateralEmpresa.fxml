                                <Insets left="10.0" top="2.0" />
                            </HBox.margin>
                            <image>
                                <Image url="@../../images/mejora/Company.png" />
                            </image>
                        </ImageView>
                        <Label prefHeight="67.0" prefWidth="149.0" styleClass="labelLateral" text="%lateral.empresa">
