                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                    <children>
                        <ImageView fx:id="imagenPanel" fitHeight="43.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true" styleClass="empresa">
