<AnchorPane id="fondoMenuLateral" prefHeight="908.0" prefWidth="210.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.mejora.ControllerMenuLateralEmpresa">
